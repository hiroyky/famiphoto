type Photo implements Node {
    id: ID!
    ownerId: ID!
    owner: User!
    groupId: ID!
    group: Group!
    name: String!
    importedAt: Timestamp!
    dateTimeOriginal: Timestamp!
    previewUrl: String!
    thumbnailUrl: String!
    exifData: [PhotoExif!]!
    fileTypes: [String!]!
    files: [PhotoFile!]!
}

type PhotoPagination implements Pagination {
    pageInfo: PaginationInfo!
    nodes: [Photo!]!
}