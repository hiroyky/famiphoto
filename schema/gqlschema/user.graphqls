type User implements Node {
    id: ID!
    userId: String!
    name: String!
    status: UserStatus!
    password: UserPassword
    belongGroups: [Group!]!
}

enum UserStatus {
    Active
    Withdrawal
}

type UserEdge implements Edge {
    cursor: Cursor!
    node: User!
}

type UserPagination implements Pagination {
    pageInfo: PaginationInfo!
    nodes: [User!]!
}
