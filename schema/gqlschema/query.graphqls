type Query {
    gqlStatus: GqlStatus!
    user(id: ID!): User
    users(id: ID, limit: Int, offset: Int): UserPagination!
    existUserId(id: String!): Boolean!
    me: User
    photo(id: ID!): Photo
    photos(
        id: ID,
        limit: Int,
        offset: Int,
        year: Int,
        month: Int,
        date: Int,
    ): PhotoPagination!
    photoFile(id: ID!): PhotoFile
    photoFiles(photoId: ID!): [PhotoFile!]!
}
