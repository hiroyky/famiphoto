type Group implements Node {
    id: ID!
    groupId: String!
    name: String!
    userPagination(limit: Int, offset: Int): UserPagination!
}

type GroupEdge implements Edge {
    cursor: Cursor!
    node: Group!
}

type GroupPagination implements Pagination {
    pageInfo: PaginationInfo!
    nodes: [Group!]!
}
